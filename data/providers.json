{
  "aip": {
    "name": "American Institute of Physics",
    "url": "https://aip.scitation.org/action/quickLink",
    "method": "GET",
    "fields": "?quickLinkJournal=$JOURNAL&quickLinkVolume=$VOLUME&quickLinkPage=$PAGE&quickLink=true"
  },
  "acs": {
    "name": "American Chemical Society",
    "url": "https://pubs.acs.org/action/quickLink",
    "method": "GET",
    "fields": "?quickLinkJournal=$JOURNAL&quickLinkVolume=$VOLUME&quickLinkPage=$PAGE&quickLink=true"
  },
  "sd": {
    "name": "ScienceDirect (Elsevier)",
    "method": "GET",
    "url": "https://www.sciencedirect.com/search/advanced",
    "fields": "?cid=$JOURNAL&volume=$VOLUME&page=$PAGE"
  },
  "sl": {
    "name": "SpringerLink (Springer)",
    "method": "GET",
    "url": "https://link.springer.com/journal/volumesAndIssues/",
    "fields": "$JOURNAL#volume$VOLUME"
  },
  "nature": {
    "name": "Nature",
    "method": "GET",
    "url": "https://www.nature.com/search",
    "fields": "?volume=$VOLUME&spage=$PAGE&order=relevance&journal=$JOURNAL"
  }
}